{
  "name": "hallidayspointtavern",
  "private": true,
  "version": "1.0.0",
  "author": "Phiranno Designs <services@phirannodesigns.com.au>",
  "main": "package.json",
  "scripts": {
    "build": "sanity build",
    "deploy": "npm run deploy:studio && npm run deploy:graphql",
    "deploy:graphql": "sanity graphql deploy --playground",
    "deploy:studio": "sanity deploy",
    "dev": "sanity start",
    "eslint": "eslint --color --ext .js,.json,.jsx,.ts,.tsx schema",
    "eslint:fix": "npm run eslint -- --fix"
  },
  "dependencies": {
    "@heroicons/react": "^1.0.1",
    "@sanity/base": "^2.10.0",
    "@sanity/components": "^2.2.6",
    "@sanity/core": "^2.10.0",
    "@sanity/default-layout": "^2.10.0",
    "@sanity/default-login": "^2.8.0",
    "@sanity/desk-tool": "^2.10.1",
    "@sanity/vision": "^2.10.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.2.0"
  },
  "devDependencies": {
    "eslint": "^7.25.0",
    "eslint-config-adjunct": "^4.11.1",
    "eslint-config-auto": "^0.7.1",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.2.1"
  },
  "eslintConfig": {
    "extends": [
      "auto"
    ],
    "root": true,
    "rules": {
      "react/jsx-filename-extension": 0
    }
  },
  "prettier": {
    "endOfLine": "lf",
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false
  }
}
